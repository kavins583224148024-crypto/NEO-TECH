a=float(input("Enter first value:"))
b=float(input("Enter second value:"))
result=a**2+b**2+2*a*b
print("(a+b)^2=",result)
a=float(input("Enter the first value:"))
b=float(input("Enter the second value:"))
result=a**2+b**2+2*a*b
print("(a+b)^2=",result)
result=a**2+b**2-2*a*b
print("(a-b)^2=",result)
